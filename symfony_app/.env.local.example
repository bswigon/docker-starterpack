# Skopiuj ten plik do .env.local i dostosuj wartości

# Wygeneruj nowy secret: php bin/console secrets:generate-keys
APP_SECRET=CHANGE_ME_TO_RANDOM_STRING

# Konfiguracja bazy danych (domyślnie z docker-compose.yml)
DATABASE_URL="mysql://symfony_user:symfony_password@database:3306/symfony_db?serverVersion=8.0&charset=utf8mb4"

# Redis
REDIS_URL='redis://redis'

# RabbitMQ
MESSENGER_TRANSPORT_DSN='amqp://guest:guest@rabbitmq:5672/%2f/messages'
